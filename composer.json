{
    "name": "justbetter/laravel-magento-products",
    "description": "Package to store product data in a local DB",
    "type": "package",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "justbetter/laravel-magento-client": "^1.1.0",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "nunomaduro/larastan": "^2.5",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.0"
    },
    "authors": [
        {
            "name": "Vincent Boon",
            "email": "vincent@justbetter.nl",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "JustBetter\\MagentoProducts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustBetter\\MagentoProducts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan",
        "style": "pint --test",
        "quality": [
            "@test",
            "@analyse",
            "@style"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustBetter\\MagentoProducts\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
